{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://app.homy.health",
    "NEXT_PUBLIC_APP_ENV": "production",
    "NEXT_PUBLIC_ENABLE_AI_FEATURES": "true",
    "NEXT_PUBLIC_ENABLE_CLEANSHOT": "true"
  },
  "github": {
    "silent": true,
    "autoAlias": true
  }
}
